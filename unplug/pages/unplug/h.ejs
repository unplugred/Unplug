<%- include('../partials/header',{title:"h"}) %>
		<style>
			body {
				margin: 30vmin;
				text-align: justify;
				height: calc(100vh - 60vmin);
				width: calc(100vw - 60vmin);
				background-repeat: no-repeat;
				background-position: center center;
			}
		</style>
	</head>
	<body>
		<script>
			var array = new Uint16Array(2000);
			window.crypto.getRandomValues(array);
			var str = '';
			for (var i = 0; i < array.length; i++) {
				str += String.fromCharCode(array[i]);
			};
			document.body.innerHTML = str;

			function bg() {
				document.body.style.backgroundImage = "url('<%= assets %>/h/" + Math.floor(Math.random()*5) + ".png')";
				setTimeout(bg, Math.random()*200 + 10);
			}
			bg();
		</script>
<%- include('../partials/footer') %>
