			body {
				display: flex;
				font-family: monospace;
				font-size: 13px;
				color: #353535;
				overflow-y: auto;
				scrollbar-width: thin;
				scrollbar-color: #353535 transparent;
			}
			#projectslist {
				margin: auto;
				display: inline-block;
				padding-top: 5px;
				padding-bottom: 30px;
				padding-left: 14px;
				max-width: 800px;
			}
			#title {
				background: white;
				display: inline-block;
				padding: 3px 4px;
				margin: 6px -10px;
				font-size: 20px;
			}
			.project {
				display: inline;
				background: white;
				padding-right: 4px;
			}
			.projectlink, .projecttime, .projectdesc {
