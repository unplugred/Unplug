<%- include('../partials/header',{title:"projects",color:"#dfdfdf"}) %>
		<style>
			body {
				display: flex;
				font-family: monospace;
				font-size: 13px;
				color: #353535;
				overflow-y: auto;
			}
			#projectslist {
				margin: auto;
				display: inline-block;
				padding-top: 5px;
				padding-bottom: 30px;
				padding-left: 14px;
			}
			#title {
				background: white;
				display: inline-block;
				padding: 3px 4px;
				margin: 6px -10px;
