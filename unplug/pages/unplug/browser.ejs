<%- include('../partials/header',{title:"unplug"}) %>
		<style>
			#win-browser {
				width: 100vw;
				height: 100vh;
			}
		</style>
		<link rel=StyleSheet href="<%= assets %>/inception/browser.css?v=3" type="text/css" media=screen />
	</head>
	<body>
		<div id="win-browser" class="win-pos">
			<div class="win-frame">
				<div id="win-header">unplug</div>
				<div id="win-dragbar"></div>
			</div>
			<div class="win-inner">
				<div id="win-rainbow"></div>
				<div id="win-topbar">
					<button class="win-button win-off" id="win-back" onclick="backk()">
						<div class="win-icon"></div>
					</button>
					<button class="win-button win-off" id="win-front" onclick="frontt()">
						<div class="win-icon"></div>
					</button>
					<div class="win-insetborder1" id="win-adress">
						<input class="win-insetborder2" id="win-adress-inner" spellcheck="false"></input>
					</div>
					<button class="win-button" id="win-go" onclick="changeurl()">
						<div class="win-icon"></div>
					</button>
				</div>
				<div class="win-insetborder1" id="win-innerbrowser">
					<iframe class="win-insetborder2" id="win-iframe" src="/unplug" onLoad="updateurl(this.contentWindow.location);"></iframe>
				</div>
			</div>
		</div>
		<script type="text/javascript" src="<%= assets %>/inception/browser.js?v=2"></script>
<%- include('../partials/footer') %>