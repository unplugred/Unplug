<%- include('../partials/header',{title:"autobiography",css:false,color:"#0000aa"}) %>
		<style>
			body, html {
				width: 100vw;
				height: 100vh;
				margin: 0;
				padding: 0;
				overflow: hidden;
			}
			body {
				display: flex;
				background: url(data:image/png;base64,) top left repeat, black;
			}
			::selection {
				color: #0000aa;
				background: #F4F4F4FE;
			}
			* {
				cursor: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAsAAAATCAQAAADsID9bAAAAPklEQVR42p3QMQrAMAwEwf3/pydlwL40UbkjEChawwTCygtoAS2gBbSAFtAFR34nEuskdQDUyN9v2Pn/PlUPcRSXac4QlqQAAAAASUVORK5CYII=) 0 0, default;
			}
			#wrap {
				height: calc(40vh + 40vmin);
				width: calc(40vw + 40vmin);
				position: relative;
				z-index: 20;
				margin: auto;
			}
			#text {
				overflow-y: auto;
				color: #F4F4F4;
				font-family: "FreeMono", "Courier New", monospace;
				font-weight: normal;
				background: url(data:image/png;base64,) top left no-repeat, #0000aa;
				padding-left: 16px;
				word-break: break-all;
				font-size: 16px;
				position: relative;
				width: 100%;
				height: 100%;
				top: -100%;
				left: 0;
			}
			#shadow {
				padding-left: 16px;
				position: relative;
				top: 16px;
				left: 16px;
				z-index: -20;
				width: 100%;
				height: 100%;
				background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQAQAAAAA3iMLMAAAAEElEQVR4AWNYtYohNJR4EgCGtw/xaZlxDQAAAABJRU5ErkJggg==);
				background-attachment: fixed;
				opacity: .05;
			}
			b {
				color: #0000aa;
				background-color: #F4F4F4;
			}
			.inner {
				font-style: normal;
				white-space: pre-wrap;
				tab-size: 4;
			}
			::-webkit-scrollbar-thumb {
				background-color: #F4F4F4;
			}
			::-webkit-scrollbar {
				width: 3px;
				height: 3px;
			}
		</style>
	</head>
	<body>
		<div id="wrap">
			<div id="shadow"></div>
			<div id="text">
				<i class="inner"></i><b class="inner"></b><i class="inner"></i>
			</div>
		</div>
		<script>
			//hello there.
			var txt = "<!DOCTYPE html>\n"+document.documentElement.outerHTML
				.replace("<head>","\n\t<head>")
				.replace("<\/body>","\n\t<\/body>\n");
			var index = txt.indexOf("autobiography");
			var txtdiv = document.getElementById("text");
			var divs = document.getElementsByClassName("inner");
			var reader = 0;
			var onbottom = true;
			var selected = "â–ˆ";
			divs[0].innerText = selected;
			var prevscrolltop = txtdiv.scrollTopMax;
			function update(){
				if(prevscrolltop != txtdiv.scrollHeight) {
					if(onbottom) txtdiv.scrollTop = txtdiv.scrollHeight;
					prevscrolltop = txtdiv.scrollHeight;
				} else {
					onbottom = (txtdiv.scrollTop+txtdiv.clientHeight) == txtdiv.scrollHeight;
				}

				if(reader < txt.length) {
					if(reader < index) {
						divs[0].innerText = txt.substring(0,++reader) + selected;
					} else if(reader < index+13) {
						divs[0].innerText = txt.substring(0,index);
						divs[1].innerText = txt.substring(index,++reader);
						divs[2].innerText = selected;
					} else {
						divs[2].innerText = txt.substring(index+13,++reader) + selected;
					}
					setTimeout(update,txt.charAt(reader) == "\n" ? 100 : 10);
				} else divs[2].innerText = txt.substring(index+13);
			};
			setTimeout(update,400);
		</script>
<%- include('../partials/footer') %>
